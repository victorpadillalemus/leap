<div class="filter-container">
  <h2>Date</h2>
  <div class="wrap">
    <%= simple_form_for :search, url: experiences_path, method: :get do |f| %>
    <%= f.input :transit_date, as: :string, required: false, input_html: {class: "datepicker"} %>


    <div class="slider">
      <input type="range" min="1" max="100" value="50" class="slider" id="myRange">
    </div>

    <div class="slider">
      <input type="range" min="1" max="100" value="50" class="slider" id="myRange">
    </div>
    <div id="demo"></div>


  <div class="number">
    <span class="minus">-</span>
    <input type="text" value="1"/>
    <span class="plus">+</span>
  </div>

  <%= f.submit "Search", class: "primary-button" %>
  <% end %>

</div>
